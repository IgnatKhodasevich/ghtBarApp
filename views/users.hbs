
{{#if users}}
<table class="table">
    <thead class="thead-dark">
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Username</th>
        <th scope="col">E-mail</th>
        <th scope="col">Password</th>
        <th scope="col">Admin</th>
    </tr>
    </thead>
    <tbody>
    {{#each users }}
    <tr>
        <th scope="row">{{this.id}}</th>
        <td><input type="text" value="{{this.username}}" class="form-control" disabled title="Username"/></td>
        <td>{{this.mail}}</td>
        <td>{{this.password}}</td>
        <td>{{this.isAdmin}}</td>

    </tr>
    {{/each}}
    </tbody>
</table>
{{/if}}
<div class="btn-group">
    <a href="/addItemPage" class="btn btn-outline-success" name="addButton">Add bottle</a>
    <a href="/users" class="btn btn btn-outline-primary" name="usersButton">Users</a>
    <a href="/logout" class="btn btn btn-outline-secondary" name="logoutButton">Log out</a>
</div>